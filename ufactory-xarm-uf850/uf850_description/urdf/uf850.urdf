<robot name="uf850">
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://uf850_description/meshes/link_base.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <link name="link1">
    <visual>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://uf850_description/meshes/link1.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_link1" type="fixed">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
  </joint>

  <link name="link2">
    <visual>
      <origin xyz="0 0 0" rpy="0 -1.57 1.57"/> <!-- keep neutral -->
      <geometry>
        <mesh filename="package://uf850_description/meshes/link2.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link1_to_link2" type="fixed">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/> <!-- rotate link2 90° about Y -->
  </joint>

  <link name="link3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.57"/> <!-- keep neutral -->
      <geometry>
        <mesh filename="package://uf850_description/meshes/link3.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link2_to_link3" type="fixed">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0.4" rpy="1.57 1.57 3.14"/> <!-- rotate link2 90° about Y -->
  </joint>

  <link name="link4">
    <visual>
      <origin xyz="0.3 0 0" rpy="1.57 -1.57 1.57"/> <!-- keep neutral -->
      <geometry>
        <mesh filename="package://uf850_description/meshes/link4.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link3_to_link4" type="fixed">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0.15 -0.15 0" rpy="1.57 0 0"/> <!-- rotate link2 90° about Y -->
  </joint>

  <link name="link5">
    <visual>
      <origin xyz="0 0.1 0" rpy="1.57 1.57 3.14"/> 
      <geometry>
        <mesh filename="package://uf850_description/meshes/link5.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link4_to_link5" type="fixed">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0.3 0.1 0" rpy="3.14 0 0"/> 
  </joint>

  <link name="link6">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <mesh filename="package://uf850_description/meshes/link6.stl" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.7 0.7 0.7 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="link5_to_link6" type="fixed">
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0.1 0.1 0" rpy="0 1.57 0"/> 
  </joint>
</robot>
